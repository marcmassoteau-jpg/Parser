{
  "name": "visual-parser-module",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "High-performance visual parser for CSV, Fixed Width, FIN, ISO 20022 and custom formats with WASM acceleration",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/assets/index.css",
    "./service": {
      "import": "./dist/parsers/ParserService.js",
      "types": "./dist/parsers/ParserService.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:full": "npm run build:wasm && tsc && vite build",
    "build:lib": "npm run build:wasm && tsc && vite build --mode lib",
    "build:wasm": "cd rust && wasm-pack build --target web --out-dir ../src/wasm --out-name parser_wasm",
    "build:wasm:release": "cd rust && wasm-pack build --target web --release --out-dir ../src/wasm --out-name parser_wasm",
    "build:wasm:dev": "cd rust && wasm-pack build --target web --dev --out-dir ../src/wasm --out-name parser_wasm",
    "clean": "rm -rf dist src/wasm rust/target",
    "clean:wasm": "rm -rf src/wasm rust/target",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-pattern src/wasm",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "playwright:install": "playwright install --with-deps",
    "benchmark": "node scripts/benchmark.js",
    "setup:rust": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh && cargo install wasm-pack"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "clsx": "^2.1.0",
    "fast-xml-parser": "^4.3.2",
    "framer-motion": "^10.18.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.6",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.2.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "homepage": "https://marcmassoteau-jpg.github.io/Parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcmassoteau-jpg/Parser"
  },
  "keywords": [
    "parser",
    "csv",
    "swift",
    "fin",
    "iso20022",
    "fixed-width",
    "visual",
    "react",
    "data-mapping",
    "wasm",
    "high-performance",
    "streaming",
    "web-worker"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
